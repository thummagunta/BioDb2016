   "source": [
    "from Bio.Seq import Seq\n",
    "import Bio.Alphabet\n",
    "\n",
    "f = open(\"rosalind_prot (1).txt\", 'r')\n",
    "seq = Seq(f.read().replace('\\n', ''), Bio.Alphabet.IUPAC.unambiguous_rna)\n",
    "f.close\n",
    "\n",
    "protein = seq.translate(to_stop=True)\n",
    "\n",
    "o = open(\"PROT.txt\", 'w')\n",
    "o.write(str(protein))\n",
    "o.close()"
   ]
